service vendor.qseecomd /vendor/bin/linker64_qti /vendor/bin/qseecomd
    class core
    user root
    group root
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib64/hw:/sbin
    disabled
    seclabel u:r:recovery:s0

service vendor.keymint-qti /vendor/bin/linker64_qti /vendor/bin/hw/android.hardware.security.keymint-service-qti
    class early_hal
    user root
    group root drmrpc
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib64/hw:/sbin
    disabled
    seclabel u:r:recovery:s0

service vendor.gatekeeper /vendor/bin/linker64_qti /vendor/bin/hw/android.hardware.gatekeeper-service-qti
    class late_hal
    user root
    group root
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib64/hw:/sbin
    disabled
    seclabel u:r:recovery:s0

on property:hwservicemanager.ready=true
    start vendor.qseecomd
    start vendor.keymint-qti
    start vendor.gatekeeper

on property:ro.crypto.state=encrypted
    start vendor.qseecomd
    start vendor.keymint-qti
    start vendor.gatekeeper