on init
    start boot-hal-1-2
    chmod 0755 /vendor/bin/linker64_qti
    chmod 0755 /vendor/bin/qseecomd
    chmod 0755 /vendor/bin/hw/android.hardware.security.keymint-service-qti

on fs
    wait /dev/block/platform/soc/${ro.boot.bootdevice}
    symlink /dev/block/platform/soc/${ro.boot.bootdevice} /dev/block/bootdevice
    write /proc/sys/kernel/firmware_config/force_sysfs_fallback 1
    chmod 0660 /dev/qseecom
    chown system drmrpc /dev/qseecom
    chmod 0664 /dev/ion
    chown system system /dev/ion
    mkdir /mnt/vendor 0775 shell system
    mkdir /mnt/vendor/persist 0771 system system
    chown system system /mnt/vendor/persist
    chmod 0771 /mnt/vendor/persist
    restorecon /mnt/vendor/persist
    mount ext4 /dev/block/by-name/persist /mnt/vendor/persist rw
    mount ext4 /dev/block/by-name/persist /persist rw
    mkdir /persist/OFRPFox

